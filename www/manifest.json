{
  "name": "Measure.app",
  "description": "Open Internet Measurement",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["js/support/chromeAppLauncher.js"]
    }
  },
  "icons": { "16": "img/icon16.png",
             "32": "img/icon32.png",
             "48": "img/icon48.png",
             "128": "img/icon128.png" },
  "permissions": [
    "storage",
	"alarms",
    "unlimitedStorage"
  ]
}
